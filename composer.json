{
    "name": "jbenoit/php-rs232",
    "description": "Library to connect to RS232 devices",
    "type": "library",
    "license": "LGPL-3.0",
    "keywords": [
        "php",
        "serial",
        "rs232",
        "rs-232",
        "eia232",
        "eia-232",
        "tia232",
        "tia-232",
        "library"
        ],
    "authors": [
        {
            "name": "Julien BENOIT",
            "email": "jbenoit.jb+dev@gmail.com",
            "homepage": "http://www.julienbenoit.net",
            "role": "Developer"
        }
    ],"support": {
        "issues": "http://github.com/jbenoit2011/php-rs232/issues",
        "source": "http://github.com/jbenoit2011/php-rs232"
    },
    "require": {
        "zendframework/zend-stdlib": "~2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "4.6.*"
    },
    "autoload": {
        "psr-4": {
            "Jbenoit\\PhpRs232\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jbenoit\\PhpRs232\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
